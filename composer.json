{
  "name": "mylistryx/yii2-remix-icons",
  "description": "Yii2 Remix icons asset bundle",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "yii2-extension",
  "authors": [
    {
      "name": "Sergey Zhukovskiy",
      "email": "mylistryx@gmail.com",
      "homepage": "https://net23.ru"
    }
  ],
  "keywords": [
    "yii2",
    "remix",
    "icons",
    "asset",
    "bundle"
  ],
  "version": "4.8.0",
  "readme": "README.md",
  "autoload": {
    "psr-4": {
      "yii\\ri\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=8.0",
    "yiisoft/yii2": "~2.0.0"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
